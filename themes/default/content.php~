$path=path_info();
if ($path[1]=="setting") {
	echo "<div id='seting'></div>";
	//setting();
}
else {
?>
		<h3><a href='#'>
		<?php
			if ($path[2] == ""){
			echo "Home";	
			}
			else {
			echo ucfirst($path[2]);
			}
		?>
		</a></h3>
		<div><center>
		<?php 
			if($path[1]=="report") {
				getplugin_modul_report($path[2]);
			}
		?>
		</center></div>
<?php } ?>
